        "^.github/dependabot.yml$",
        "^.github/ISSUE_TEMPLATE/",
        "^docs/",
        "^catalog-info.yaml$",
        "^CODE_OF_CONDUCT.md$",
        "^.buildkite/pipeline.schedule-daily.yml$",
        "^.buildkite/pipeline.schedule-weekly.yml$",
        "^.buildkite/pipeline.backport.yml$",
        "^.buildkite/pull-requests.json$"

pf-elastic-symbolizer:
    host: {{host}}
    verbose: {{verbose}}
    metrics:
        prometheus_host: {{metrics_prometheus_host}}
        expvar_host: {{metrics_expvar_host}}
    {{#if memory_limit}}
    memory_limit: {{memory_limit}}
    {{/if}}
    {{#if endpoint}}
    endpoint: {{endpoint}}
    {{/if}}
    ssl:
        enabled: {{tls_enabled}}
        certificate: {{tls_certificate_path}}
        key: {{tls_key_path}}
        key_passphrase: {{tls_key_passphrase}}
        supported_protocols:
        {{#each tls_supported_protocols}}
          - {{this}}
        {{/each}}
        cipher_suites:
        {{#each tls_cipher_suites}}
          - {{this}}
        {{/each}}
        curve_types:
        {{#each tls_curve_types}}
          - {{this}}
        {{/each}}

connectors:
    forward: {}
exporters:
    elasticsearch/componentid-0:
        endpoints:
            - https://elasticsearch:9200
extensions:
    basicauth/componentid-0:
        htpasswd:
            file: /etc/otel/.htpasswd
inputs: []
output_permissions:
    default:
        _elastic_agent_checks:
            cluster:
                - monitor
        _elastic_agent_monitoring:
            indices: []
        uuid-for-permissions-on-related-indices:
            indices:
                - names:
                    - logs-*-*
                  privileges:
                    - auto_configure
                    - create_doc
                - names:
                    - metrics-*-*
                  privileges:
                    - auto_configure
                    - create_doc
                - names:
                    - traces-*-*
                  privileges:
                    - auto_configure
                    - create_doc
processors:
    resourcedetection/componentid-0:
        detectors:
            - system
    transform/componentid-1:
        log_statements:
            - context: log
              statements:
                - set(attributes["data_stream.type"], "logs")
                - set(attributes["data_stream.dataset"], "otlpreceiver")
                - set(attributes["data_stream.namespace"], "ep")
        metric_statements:
            - context: datapoint
              statements:
                - set(attributes["data_stream.type"], "metrics")
                - set(attributes["data_stream.dataset"], "otlpreceiver")
                - set(attributes["data_stream.namespace"], "ep")
        trace_statements:
            - context: span
              statements:
                - set(attributes["data_stream.type"], "traces")
                - set(attributes["data_stream.dataset"], "otlpreceiver")
                - set(attributes["data_stream.namespace"], "ep")
            - context: spanevent
              statements:
                - set(attributes["data_stream.type"], "logs")
                - set(attributes["data_stream.namespace"], "ep")
receivers:
    otlp/componentid-0:
        protocols:
            grpc:
                auth:
                    authenticator: basicauth
                endpoint: localhost:4317
            http:
                auth:
                    authenticator: basicauth
                endpoint: localhost:4318
secret_references: []
service:
    extensions:
        - basicauth
    pipelines:
        logs:
            exporters:
                - elasticsearch/componentid-0
            receivers:
                - forward
        logs/componentid-0:
            exporters:
                - forward
            processors:
                - resourcedetection/componentid-0
                - transform/componentid-1
            receivers:
                - otlp/componentid-0
        metrics:
            exporters:
                - elasticsearch/componentid-0
            receivers:
                - forward
        metrics/componentid-1:
            exporters:
                - forward
            processors:
                - resourcedetection/componentid-0
                - transform/componentid-1
            receivers:
                - otlp/componentid-0
        traces:
            exporters:
                - elasticsearch/componentid-0
            receivers:
                - forward
        traces/componentid-2:
            exporters:
                - forward
            processors:
                - resourcedetection/componentid-0
                - transform/componentid-1
            receivers:
                - otlp/componentid-0

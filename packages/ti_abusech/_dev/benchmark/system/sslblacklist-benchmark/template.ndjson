{{- $timestamp := generate "timestamp" -}}
{{- $duration_start := generate "duration_start" -}}
{{- $ts := ($timestamp | date_modify (print $duration_start)).Format "2006-01-02 15:04:05" -}}
{{- $sha1 := sha1sum (generate "sha1") }}
{{- $reason := generate "reason" -}}
{{$ts}},{{$sha1}},{{$reason}}

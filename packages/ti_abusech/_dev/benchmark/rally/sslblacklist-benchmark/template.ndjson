{{- $timestamp := generate "timestamp" -}}
{{- $expireDur := generate "expire_dur" -}}
{{- $hostname := generate "host.name" -}}
{{- $agentId := generate "agent.id" -}}
{{- $id := generate "id" -}}
{{- $duration_start := generate "duration_start" -}}
{{- $ts := ($timestamp | date_modify (print $duration_start)).Format "2006-01-02T15:04:05Z" -}}
{{- $sha1 := sha1sum (generate "sha1") }}
{{- $reason := generate "reason" -}}
{
  "@timestamp": "{{ $timestamp.Format "2006-01-02T15:04:05.999999Z07:00" }}",
  "_conf": {
    "interval": "{{ $expireDur }}h"
  },
  "agent": {
    "id": "{{$agentId}}",
    "name": "{{$hostname}}",
    "type": "filebeat",
    "version": "8.19.0",
    "ephemeral_id": "{{$agentId}}"
  },
  "data_stream": {
    "namespace": "ep",
    "type": "logs",
    "dataset": "ti_abusech.sslblacklist"
  },
  "elastic_agent": {
    "id": "60a22c30-4c80-4e95-b5b1-87ff540d8afd",
    "snapshot": false,
    "version": "8.19.0"
  },
  "message": "{\"reason\":\"{{$reason}}\",\"sha1\":\"{{$sha1}}\",\"ts\":\"{{$ts}}\"}",
  "event": {
    "dataset": "ti_abusech.sslblacklist"
  },
  "input": {
    "type": "cel"
  },
  "tags": [
    "preserve_original_event",
    "forwarded",
    "abusech-sslblacklist"
  ]
}

metricsets: ["jmx"]
namespace: "metrics"
hosts: {{hosts}}
http_method: "GET"
path: {{path}}
username: {{username}}
password: {{password}}
period: {{period}}
tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
{{#if processors}}
processors:
{{processors}}
{{/if}}
jmx.mappings:  
  - mbean: 'java.lang:name=*,type=GarbageCollector'
    attributes:
    - attr: LastGcInfo
      field: last_info
    - attr: Name
      field: name     

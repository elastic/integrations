metricsets: ["jmx"]
namespace: "metrics"
hosts: {{hosts}}
http_method: "GET"
path: {{path}}
username: {{username}}
password: {{password}}
period: {{period}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
jmx.mappings:  
  - mbean: 'java.lang:name=PS MarkSweep,type=GarbageCollector'
    attributes:
    - attr: LastGcInfo
      field: last_gc_info    
    - attr: CollectionTime
      field: collection.time
    - attr: CollectionCount
      field: collection.count
    - attr: Valid
      field: valid
    - attr: Name
      field: name
      
metricsets: ["jmx"]
namespace: "metrics"
hosts: {{hosts}}
http_method: "GET"
path: {{path}}
username: {{username}}
password: {{password}}
period: {{period}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
jmx.mappings:  
  - mbean: 'java.lang:name=PS MarkSweep,type=GarbageCollector'
    attributes:
    - attr: LastGcInfo
      field: last_gc_info    
    - attr: CollectionTime
      field: collection.time
    - attr: CollectionCount
      field: collection.count
    - attr: Valid
      field: valid
    - attr: Name
      field: name
  - mbean: 'java.lang:name=PS Old Gen,type=MemoryPool'
    attributes:
    - attr: Usage
      field: memory_pool.usage
    - attr: UsageThresholdCount
      field: memory_pool.usage_threshold.count
    - attr: UsageThresholdSupported
      field: memory_pool.usage_threshold.supported
    - attr: UsageThreshold
      field: memory_pool.usage_threshold.threshold
    - attr: UsageThresholdExceeded
      field: memory_pool.usage_threshold.exceeded
    - attr: PeakUsage
      field: memory_pool.peak_usage
    - attr: CollectionUsage
      field: memory_pool.collection_usage
    - attr: CollectionUsageThresholdCount
      field: memory_pool.collection_usage_threshold.count
    - attr: CollectionUsageThreshold
      field: memory_pool.collection_usage_threshold.threshold
    - attr: CollectionUsageThresholdSupported
      field: memory_pool.collection_usage_threshold.supported
    - attr: CollectionUsageThresholdExceeded
      field: memory_pool.collection_usage_threshold.exceeded
    - attr: Name
      field: memory_pool.name
    - attr: Type
      field: memory_pool.type
    - attr: Valid
      field: memory_pool.valid
      
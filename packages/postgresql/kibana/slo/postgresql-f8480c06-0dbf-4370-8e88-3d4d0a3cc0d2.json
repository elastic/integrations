{
    "attributes": {
        "name": "PostgreSQL deleted row rate",
        "description": "This SLO monitors the rate of rows deleted in PostgreSQL databases. It ensures that the number of rows deleted remains below a specified threshold to maintain database integrity and performance.",
        "indicator": {
            "type": "sli.kql.custom",
            "params": {
                "index": "metrics-*",
                "filter": "data_stream.dataset : \"postgresql.database\" ",
                "good": "postgresql.database.rows.deleted < 100",
                "total": "postgresql.database.rows.deleted : *",
                "timestampField": "@timestamp",
                "dataViewId": "metrics-*"
            }
        },
        "budgetingMethod": "occurrences",
        "timeWindow": {
            "duration": "7d",
            "type": "rolling"
        },
        "objective": {
            "target": 0.99
        },
        "tags": [],
        "groupBy": [
            "*"
        ],
        "settings": {
            "preventInitialBackfill": false
        }
    },
    "id": "postgresql-f8480c06-0dbf-4370-8e88-3d4d0a3cc0d2",
    "references": [],
    "type": "slo"
}
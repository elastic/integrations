{
    "attributes": {
        "name": "PostgreSQL database deadlocks rate",
        "description": "This SLO monitors the rate of deadlocks in PostgreSQL databases. This SLO aims to minimize the number of deadlocks to ensure smooth database operations.",
        "indicator": {
            "type": "sli.kql.custom",
            "params": {
                "index": "metrics-*",
                "filter": "data_stream.dataset : \"postgresql.database\" ",
                "good": "postgresql.database.deadlocks < 100",
                "total": "postgresql.database.deadlocks : * ",
                "timestampField": "@timestamp",
                "dataViewId": "metrics-*"
            }
        },
        "budgetingMethod": "occurrences",
        "timeWindow": {
            "duration": "7d",
            "type": "rolling"
        },
        "objective": {
            "target": 0.99
        },
        "tags": [],
        "groupBy": [
            "*"
        ],
        "settings": {
            "preventInitialBackfill": false
        }
    },
    "id": "postgresql-88bcc0ac-ada5-4917-af3b-9197fedb3452",
    "references": [],
    "type": "slo"
}
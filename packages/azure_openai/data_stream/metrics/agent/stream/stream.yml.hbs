            timegrain: "PT1M"
            ignore_unsupported: true
            dimensions:
            - name: Region
              value: '*'
            - name: ModelDeploymentName
              value: '*'
            - name: ModelName
              value: '*'
            - name: ModelVersion
              value: '*'

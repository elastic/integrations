listen_address: {{listen_address}}
listen_port: {{listen_port}}
url: {{url}}
prefix: cybersixgill
{{#if preserve_original_event}}
preserve_original_event: true
{{/if}}
{{#if username}}
basic_auth: true
username: "{{username}}"
password "{{password}}"
{{/if}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
tags:
{{#if preserve_original_event}}
 - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
 - {{tag}}
{{/each}}
{{#contains tags "forwarded"}}
publisher_pipeline.disable_host: true
{{/contains}}
processors:
  - add_locale: ~

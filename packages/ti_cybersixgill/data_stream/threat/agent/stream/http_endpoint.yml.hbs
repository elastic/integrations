listen_address: {{listen_address}}
listen_port: {{listen_port}}
url: {{url}}
prefix: json
{{#if preserve_original_event}}
preserve_original_event: true
{{/if}}
{{#if username}}
{{#if password}}
basic_auth: true
username: "{{username}}"
password: "{{password}}"
{{/if}}
{{/if}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
tags:
{{#if preserve_original_event}}
 - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
 - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if processors}}
processors:
  {{processors}}
{{/if}}

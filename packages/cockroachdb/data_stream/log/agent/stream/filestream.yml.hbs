paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}


tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if experimental_conversion}}
  - experimental_ecs_conversion
{{/if}}
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}

{{#if processors}}
processors:
{{processors}}
{{/if}}

{{#if ignore_files}}
prospector.scanner.exclude_files::
{{#each ignore_files as |file|}}
  - {{file}}
{{/each}}
{{/if}}

prospector.scanner.fingerprint:
  enabled: true
  offset: 0
  length: 1024
  
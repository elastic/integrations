metricsets: ["collector"]
bearer_token_file: {{bearer_token_file}}
hosts:
{{#each hosts}}
    - {{this}}
{{/each}}
metrics_filters.exclude:
    - raft_process_handleready_latency
    - raft_scheduler_latency
    - sql_mem_sql_txn_max
    - raft_process_applycommitted_latency
    - raft_process_commandcommit_latency
    - raft_process_logcommit_latency
    - sql_mem_distsql_max
    - sql_mem_distsql_max
    - txn_durations
    - exec_latency
    - sql_mem_internal_txn_max
    - sql_stats_txn_stats_collection_duration
    - sql_mem_internal_session_max
metrics_filters.include:
{{#each metrics_filters.include}}
    - {{this}}
{{/each}}
metrics_path: {{metrics_path}}
password: {{password}}
period: {{period}}
ssl.certificate_authorities:
{{#each ssl.certificate_authorities}}
    - {{this}}
{{/each}}
use_types: true
username: {{username}}



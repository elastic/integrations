config_version: "2"
interval: {{interval}}
request.method: "GET"

request.url: http://{{hostname}}/admin/v2/logs/authentication

response.split:
  target: body.response.authlogs

tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{#contains tags "forwarded"}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

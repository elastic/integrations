paths:
{{#each paths}}
  - {{this}}
{{/each}}
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{#contains tags "forwarded"}}
publisher_pipeline.disable_host: true
{{/contains}}
exclude_files: [".gz$"]
processors:
- add_locale: ~
{{#if processors}}
{{processors}}
{{/if}}
id: nginx-ingress-controller-access-${kubernetes.container.id}
paths:
{{#each paths}}
  - {{this}}
{{/each}}
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
exclude_files: [".gz$"]
processors:
- add_locale: ~
{{#if processors}}
{{processors}}
{{/if}}
condition: {{ condition }}
prospector.scanner.symlinks: {{ symlinks }}
{{#if useFingerprint}}
file_identity.fingerprint: ~
prospector:
  scanner:
    fingerprint:
      {{ fingerprintYaml }}
{{/if}}
parsers:
- container:
    stream: stdout
    format: auto

paths:
{{#each paths}}
  - {{this}}
{{/each}}
exclude_files: [".gz$"]
multiline:
  pattern: '^[A-Z]{1}[0-9]{4} '
  negate: true
  match: after
processors:
- drop_event:
    when:
      not:
        regexp:
          message: '^[A-Z]{1}[0-9]{4} ' # Ignore welcome notice (not starting with log level)
- add_locale: ~

inputs:
    - data_stream:
        namespace: ep
      meta:
        package:
            name: github
      name: test-abs-github
      streams:
        - account_name: devstoreaccount1
          auth.shared_credentials.account_key: ${SECRET_0}
          containers:
            - name: test-container
          data_stream:
            dataset: github.audit
            type: logs
          file_selectors: null
          max_workers: 3
          poll: true
          poll_interval: 15s
          publisher_pipeline.disable_host: true
          tags:
            - preserve_original_event
            - preserve_duplicate_custom_fields
            - forwarded
            - github.audit
      type: azure-blob-storage
      use_output: default
output_permissions:
    default:
        _elastic_agent_checks:
            cluster:
                - monitor
        _elastic_agent_monitoring:
            indices: []
        uuid-for-permissions-on-related-indices:
            indices:
                - names:
                    - logs-github.audit-ep
                  privileges:
                    - auto_configure
                    - create_doc
secret_references:
    - {}

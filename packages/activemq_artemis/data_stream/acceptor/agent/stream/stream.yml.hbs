metricsets: ["jmx"]
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
path: {{path}}
period: {{ period }}
username: {{username}}
password: {{password}}
namespace: activemq_artemis
http_method: "GET"
tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if ssl}}
{{ssl}}
{{/if}}
{{#if processors}}
processors:
{{processors}}
{{/if}}
jmx.mappings:
    - mbean: 'org.apache.activemq.artemis:broker=*,component=acceptors,name=*'
      attributes:
        - attr: ConnectionsCreated
          field: connections.created
        - attr: ConnectionsDestroyed
          field: connections.destroyed
        - attr: Started
          field: started
        - attr: FactoryClassName
          field: factory.class_name
        - attr: Parameters
          field: parameters
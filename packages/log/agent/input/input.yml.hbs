paths:
{{#each paths}}
  - {{this}}
{{/each}}

{{#if ignore_older}}
ignore_older: {{ignore_older}}
{{/if}}
data_stream:
  dataset: {{data_stream.dataset}}
{{#if processors.length}}
processors:
{{processors}}
{{/if}}
{{#if tags.length}}
tags:
{{#each tags as |tag i|}}
- {{tag}}
{{/each}}
{{/if}}

{{custom}}

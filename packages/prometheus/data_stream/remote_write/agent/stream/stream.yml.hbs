metricsets: ["remote_write"]
host: {{host}}
port: {{port}}
ssl.enabled: {{ssl.enabled}}
ssl.certificate: {{ssl.certificate}}
ssl.key: {{ssl.key}}
{{#if ssl.certificate_authorities}}
ssl.certificate_authorities:
{{#each ssl.certificate_authorities}}
  - {{this}}
{{/each}}
{{/if}}
period: {{period}}
rate_counters: {{rate_counters}}
use_types: {{use_types}}
types_patterns.exclude:
{{#each types_patterns.counter_patterns}}
  - {{this}}
{{/each}}
types_patterns.include:
{{#each types_patterns.histogram_patterns}}
  - {{this}}
{{/each}}
data_stream:
  dataset: {{data_stream.dataset}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

inputs:
    - data_stream:
        namespace: ep
      meta:
        package:
            name: m365_defender
      name: test-default-m365_defender
      streams:
        - connection_string: ${SECRET_0}
          consumer_group: $Default
          data_stream:
            dataset: m365_defender.event
          eventhub: example
          publisher_pipeline.disable_host: true
          storage_account: default
          storage_account_container: azure-eventhub-input-example
          storage_account_key: ${SECRET_1}
          tags:
            - preserve_original_event
            - preserve_duplicate_custom_fields
            - forwarded
            - m365_defender-event
            - test-policy
      type: azure-eventhub
      use_output: default
output_permissions:
    default:
        _elastic_agent_checks:
            cluster:
                - monitor
        _elastic_agent_monitoring:
            indices: []
        uuid-for-permissions-on-related-indices:
            indices:
                - names:
                    - logs-m365_defender.event-ep
                  privileges:
                    - auto_configure
                    - create_doc
secret_references:
    - {}
    - {}

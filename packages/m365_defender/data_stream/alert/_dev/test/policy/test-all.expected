          request.timeout: 30s
          request.tracer:
            enabled: true
            filename: ../../logs/httpjson/http-request-trace-*.ndjson
            maxbackups: 5
          request.transforms:
            - set:
                target: url.params.$top
                value: 2000
            - set:
                target: url.params.$skip
                value: 0
            - set:
                default: lastUpdateDateTime ge [[formatDate (now (parseDuration "-24h"))]]
                target: url.params.$filter
                value: lastUpdateDateTime ge [[.cursor.last_update_time]]
            - set:
                target: url.params.$orderby
                value: lastUpdateDateTime asc
          request.url: https://graph.microsoft.com/v1.0/security/alerts_v2
          response.pagination:
            - set:
                do_not_log_failure: true
                fail_on_template_error: true
                target: url.params.$filter
                value: '[[.last_response.url.params.Get "$filter"]]'
            - set:
                do_not_log_failure: true
                fail_on_template_error: true
                target: url.params.$skip
                value: '[[if (eq (len .last_response.body.value) 2000)]][[add (toInt (.last_response.url.params.Get "$skip")) 2000]][[end]]'
          response.split:
            ignore_empty_value: true
            target: body.value
          tags:
            - preserve_original_event
            - preserve_duplicate_custom_fields
            - forwarded
            - m365_defender-alert
            - test-policy
      type: httpjson
      use_output: default
output_permissions:
    default:
        _elastic_agent_checks:
            cluster:
                - monitor
        _elastic_agent_monitoring:
            indices: []
        uuid-for-permissions-on-related-indices:
            indices:
                - names:
                    - logs-m365_defender.alert-ep
                  privileges:
                    - auto_configure
                    - create_doc
secret_references:
    - {}

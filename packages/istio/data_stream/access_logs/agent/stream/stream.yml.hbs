paths:
{{#each paths}}
  - {{this}}
{{/each}}
prospector.scanner.symlinks: {{ symlinks }}
parsers:
- container:
    stream: {{ containerParserStream }}
    format: {{ containerParserFormat }}
{{ additionalParsersConfig }}
tags:
{{#if preserve_original_event}}
- preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}

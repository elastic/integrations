config_version: "2"
request.method: "GET"
interval: 1m
request.url: {{api_url}}/feed/nod/?api_key={{api_key}}&api_username={{api_username}}&after=-55&app_name=westest
request.ssl:
request.timeout: "30s"
request.transforms:
  - set:
      target: header.Accept
      value: 'application/x-ndjson'
request.body:
response.decode_as: application/x-ndjson
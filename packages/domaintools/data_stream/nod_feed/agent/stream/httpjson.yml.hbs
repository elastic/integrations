config_version: "2"
request.method: "GET"
interval: {{interval}}
request.url: {{api_url}}/feed/nod/?api_key={{api_key}}&api_username={{api_username}}&sessionID=DomainToolsElasticSID&app_name=elastic_feeds&app_partner=elastic&app_version=1.0.0
request.ssl:
request.timeout: "30s"
request.transforms:
  - set:
      target: header.Accept
      value: 'application/x-ndjson'
request.body:
response.decode_as: application/x-ndjson
metricsets: ["json"]
namespace: "filebeat_input"
hosts: ["http://host.docker.internal:5066"]
period: 10s
json.is_array: true
path: "/inputs/"
tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
processors:
- convert:
    mode: rename
    ignore_missing: true
    fields:
      - from: http.filebeat_input
        to: elastic_agent.filebeat_input
{{#if processors}}
{{processors}}
{{/if}}

{{#if certificate}}certificate: {{certificate}}{{/if}}
{{#if key}}key: {{key}}{{/if}}
{{#if key_passphrase}}key_passphrase: {{key_passphrase}}{{/if}}
{{#if client_secret}}client_secret: {{client_secret}}{{/if}}
content_type:
{{#each content_type}}
 - "{{this}}"
{{/each}}
{{#if api}}
api: {{api}}
{{/if}}
tags:
{{#each tags as |tag i|}}
 - {{tag}}
{{/each}}
{{#contains tags "forwarded"}}
publisher_pipeline.disable_host: true
{{/contains}}

metricsets: ["service"]
condition: ${host.platform} == 'linux'
{{#if service.state_filter}}
service.state_filter: {{service.state_filter}}
{{/if}}
{{#if service.pattern_filter}}
service.pattern_filter: {{service.pattern_filter}}
{{/if}}
period: {{period}}
{{#if system.hostfs}}
hostfs: {{system.hostfs}}
{{/if}}
processors:
  - drop_fields:
      fields: event.module
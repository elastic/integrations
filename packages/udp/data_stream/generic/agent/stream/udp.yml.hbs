data_stream:
  dataset: {{data_stream.dataset}}
host: {{host}}:{{port}}
{{#if pipeline}}
pipeline: {{pipeline}}
{{/if}}
{{#if max_message_size}}
max_message_size: {{max_message_size}}
{{/if}}
{{#if read_buffer_size}}
read_buffer: {{read_buffer_size}}
{{/if}}
{{#if timeout}}
timeout: {{timeout}}
{{/if}}
{{#if keep_null}}
keep_null: {{keep_null}}
{{/if}}
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if processors}}
processors:
  {{processors}}
{{/if}}
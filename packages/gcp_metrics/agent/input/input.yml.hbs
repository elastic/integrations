metricsets: ["metrics"]
period: {{period}}
project_id: {{project_id}}
{{#if credentials_file}}
credentials_file_path: {{credentials_file}}
{{/if}}
{{#if credentials_json}}
credentials_json: '{{credentials_json}}'
{{/if}}
{{#if zone}}
zone: {{zone}}
{{/if}}
{{#if regions}}
regions: {{regions}}
{{/if}}
exclude_labels: {{exclude_labels}}
metrics:
  - service: {{service}}
    metric_types:
        {{#each metrics}}
        - "{{this}}"
        {{/each}}
data_stream:
  dataset: {{data_stream.dataset}}
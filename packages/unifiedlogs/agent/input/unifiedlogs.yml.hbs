condition: ${host.platform} == 'darwin'
{{#if predicate}}
predicate:
{{#each predicate as |p|}}
- {{p}}
{{/each}}
{{/if}}
{{#if start}}
start: {{start}}
{{/if}}
{{#if end}}
end: {{end}}
{{/if}}
{{#if process}}
process:
{{#each process as |p|}}
- {{p}}
{{/each}}
{{/if}}
source: {{source}}
info: {{info}}
debug: {{debug}}
backtrace: {{backtrace}}
signpost: {{signpost}}
unreliable: {{unreliable}}
mach_continuous_time: {{mach_continuous_time}}
backfill: {{backfill}}
{{#if archive_file}}
archive_file: {{archive_file}}
{{/if}}
{{#if trace_file}}
trace_file: {{trace_file}}
{{/if}}
{{#if tags.length}}
tags:
{{#each tags as |tag|}}
- {{tag}}
{{/each}}
{{#if preserve_original_event}}
- preserve_original_event
{{/if}}
{{else}}
{{#if preserve_original_event}}
tags:
- preserve_original_event
{{/if}}
{{/if}}
{{#if pipeline}}
pipeline: {{pipeline}}
{{/if}}
{{custom}}
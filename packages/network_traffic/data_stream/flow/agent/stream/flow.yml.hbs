type: flow
{{#if timeout}}
flows.timeout: '{{timeout}}'
{{/if}}
{{#if period}}
flows.period: '{{period}}'
{{/if}}
{{#if monitor_processes}}
procs:
  enabled: true
{{/if}}
{{#if interface}}
interface:
{{#if (contains ".pcap" interface)}}
  file: {{interface}}
{{else}}
  device: {{interface}}
{{/if}}
{{/if}}

metricsets: ["cloudwatch"]
period: {{period}}
{{#if data_granularity}}
data_granularity: {{data_granularity}}
{{/if}}
{{#if include_linked_accounts}}
include_linked_accounts: {{include_linked_accounts}}
{{#if owning_account}}
owning_account: "{{owning_account}}"
{{/if}}
{{/if}}
{{#if access_key_id}}
access_key_id: {{access_key_id}}
{{/if}}
{{#if secret_access_key}}
secret_access_key: {{secret_access_key}}
{{/if}}
{{#if session_token}}
session_token: {{session_token}}
{{/if}}
{{#if credential_profile_name}}
credential_profile_name: {{credential_profile_name}}
{{/if}}
{{#if shared_credential_file}}
shared_credential_file: {{shared_credential_file}}
{{/if}}
{{#if role_arn}}
role_arn: {{role_arn}}
{{/if}}
{{#if default_region}}
default_region: {{default_region}}
{{/if}}
{{#if regions}}
regions:
{{#each regions as |region i|}}
- {{region}}
{{/each}}
{{/if}}
{{#if latency}}
latency: {{latency}}
{{/if}}
{{#if tags_filter}}
tags_filter: {{tags_filter}}
{{/if}}
{{#if proxy_url }}
proxy_url: {{proxy_url}}
{{/if}}
{{#if processors}}
processors:
{{processors}}
{{/if}}
metrics:
- namespace: AWS/Bedrock-AgentCore
  resource_type: bedrock
  statistic: ["Sum"]
  name:
  - Sessions
  - Invocations
  - Errors
  - SystemErrors
  - UserErrors
  - TokenCount
  - InboundAuthorizationFailure
  - CreationCount
  - Throttles
  - ResourceAccessTokenFetchFailures
  - WorkloadAccessTokenFetchSuccess
  - InboundAuthorizationSuccess
  - ResourceAccessTokenFetchSuccess
  - WorkloadAccessTokenFetchFailures
  - TargetType.LAMBDA
  - TargetType.MCP
  - CallCount
  - ThrottleCount
  - ResourceAccessTokenFetchThrottles
  - ApiKeyFetchSuccess
  - ApiKeyFetchFailures
  - ApiKeyFetchThrottles
  - WorkloadAccessTokenFetchThrottles

- namespace: AWS/Bedrock-AgentCore
  resource_type: bedrock
  statistic: ["Average"]
  name:
  - Latency
  - Duration
  - TargetExecutionTime

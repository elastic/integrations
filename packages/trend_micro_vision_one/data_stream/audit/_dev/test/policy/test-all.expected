          cursor:
            last_update_at:
                value: '[[.last_response.url.params.Get "endDateTime"]]'
          data_stream:
            dataset: trend_micro_vision_one.audit
          interval: 5m
          processors:
            - add_fields:
                fields:
                    test: policy-test
                target: ""
          publisher_pipeline.disable_host: true

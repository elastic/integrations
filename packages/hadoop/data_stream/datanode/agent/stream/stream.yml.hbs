metricsets: ["json"]
namespace: "datanode"
hosts:
{{#each hosts}}
  - "{{this}}/jmx?qry=Hadoop:name=FSDatasetState,service=DataNode"
  - "{{this}}/jmx?qry=Hadoop:name=DataNodeActivity*,service=DataNode"
{{/each}}
period: {{period}}
path: "/"
{{#if ssl}}
{{ssl}}
{{/if}}
tags:
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{#if processors}}
processors:
{{processors}}
{{/if}}
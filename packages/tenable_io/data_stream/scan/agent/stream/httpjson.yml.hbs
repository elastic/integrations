config_version: 2
interval: {{interval}}
{{#if proxy_url}}
request.proxy_url: {{proxy_url}}
{{/if}}
{{#if ssl}}
request.ssl: {{ssl}}
{{/if}}
request.method: GET
request.url: {{hostname}}/scans
{{#if http_client_timeout}}
request.timeout: {{http_client_timeout}}
{{/if}}
request.transforms:
  - delete:
      target: header.User-Agent
  - set:
      # Follow Tenable's format: https://developer.tenable.com/docs/user-agent-header
      # NOTE: The "Build" version must be kept in sync with this package's version.
      target: header.User-Agent
      value: '[[userAgent "Integration/1.0 (Elastic; Tenable.io; Build/2.4.0)"]]'
  - set:
      target: header.X-ApiKeys
      value: 'accessKey={{access_key}}; secretKey={{secret_key}};'
response.split:
  target: body.scans
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#if preserve_duplicate_custom_fields}}
  - preserve_duplicate_custom_fields
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

period: 24h
config:
  v1:
    type: cspm
    deployment: gcp
    benchmark: cis_gcp
    gcp:
      {{#if gcp.project_id}}
      project_id: !!str {{gcp.project_id}}
      {{/if}}
      {{#if gcp.organization_id}}
      organization_id: !!str {{gcp.organization_id}}
      {{/if}}
      {{#if gcp.account_type}}
      account_type: {{gcp.account_type}}
      {{else}}
      account_type: single-account
      {{/if}}
      {{#if gcp.supports_cloud_connectors}}
      supports_cloud_connectors: {{gcp.supports_cloud_connectors}}
      {{/if}}
      credentials:
        {{#if gcp.credentials.file}}
        credentials_file_path: {{gcp.credentials.file}}
        {{/if}}
        {{#if gcp.credentials.json}}
        credentials_json: '{{gcp.credentials.json}}'
        {{/if}}
        {{#if gcp.credentials.service_account_email}}
        service_account_email: {{gcp.credentials.service_account_email}}
        {{/if}}
        {{#if gcp.credentials.audience}}
        audience: {{gcp.credentials.audience}}
        {{/if}}
{{#if condition}}
condition: {{ condition }}
{{/if}}

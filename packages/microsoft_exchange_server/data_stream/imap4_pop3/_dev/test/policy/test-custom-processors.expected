inputs:
  - data_stream:
      namespace: ep
    meta:
      package:
        name: microsoft_exchange_server
    name: test-custom-processors-microsoft_exchange_server
    streams:
      - data_stream:
          dataset: microsoft_exchange_server.imap4_pop3
        exclude_files:
          - .gz$
        paths:
          - C:\Program Files\Microsoft\Exchange Server\V15\Logging\Imap4\IMAP*.LOG
          - C:\Program Files\Microsoft\Exchange Server\V15\Logging\Pop3\POP*.LOG
        processors:
          - add_locale: null
          - add_fields:
              target: test
              fields:
                source: imap4_pop3_test
          - drop_fields:
              fields:
                - agent.ephemeral_id
        prospector.scanner: null
        tags: null
    type: filestream
    use_output: default
output_permissions:
    default:
        _elastic_agent_checks:
            cluster:
                - monitor
        _elastic_agent_monitoring:
            indices: []
        uuid-for-permissions-on-related-indices:
            indices:
                - names:
                    - logs-microsoft_exchange_server.imap4_pop3-ep
                  privileges:
                    - auto_configure
                    - create_doc
secret_references: []

paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}
exclude_files: ['\.gz$']
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if tz_offset}}
fields_under_root: true
fields:
  _conf:
    tz_offset: "{{tz_offset}}"
{{/if}}
processors:
- add_locale: ~
{{#if processors}}
{{processors}}
{{/if}}

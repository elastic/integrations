paths:
{{#each paths}}
  - {{this}}
{{/each}}
exclude_files: [".gz$"]
parsers:
- ndjson:
    add_error_key: true
    target: "kubernetes.audit"
{{#if processors}}
processors:
{{processors}}
{{/if}}
tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if condition}}
condition: {{ condition }}
{{/if}}

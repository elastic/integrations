metricsets: ["apiserver"]
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
period: {{period}}

{{#if leaderelection}}
condition: ${kubernetes_leaderelection.leader} == true
{{/if}}

{{#if bearer_token_file}}
bearer_token_file: {{bearer_token_file}}
ssl.certificate_authorities:
{{#each ssl.certificate_authorities}}
  - {{this}}
{{/each}}
{{/if}}

{{#if processors}}
processors:
{{processors}}
{{/if}}

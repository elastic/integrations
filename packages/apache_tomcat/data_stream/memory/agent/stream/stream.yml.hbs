metricsets: ["collector"]
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
metrics_filters:
  include: ["java_lang_Memory_*","java_lang_G1_Old_Generation_*"]
period: {{period}}
{{#if username}}
username: "{{username}}"
{{/if}}
{{#if password}}
password: "{{password}}"
{{/if}}
{{#if ssl}}
{{ssl}}
{{/if}}
tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if processors}}
processors:
{{processors}}
{{/if}}
config_version: 2
interval: {{interval}}
{{#if enable_request_tracer}}
resource.tracer.filename: "../../logs/cel/http-request-trace-*.ndjson"
resource.tracer.maxbackups: 5
resource.tracer.maxsize: 5
{{/if}}
{{#if proxy_url}}
resource.proxy_url: {{proxy_url}}
{{/if}}
{{#if ssl}}
resource.ssl: {{ssl}}
{{/if}}
{{#if http_client_timeout}}
resource.timeout: {{http_client_timeout}}
{{/if}}
resource.url: {{url}}
state:
  user: {{username}}
  password: {{password}}
  batch_size: {{batch_size}}
  params: {{input_parameters}}
  keep_xml: {{preserve_original_event}}
redact:
  fields:
    - password
program: |
  (has(state.worklist) && size(state.worklist) > 0 
  ?
    state
  :
    state.with(
      request("GET",
        state.?want_more.orValue(false) ?
          state.next_page.link
        :
          state.url.trim_right("/") + "/api/2.0/fo/asset/host/vm/detection/?" +
          state.?params.orValue("").parse_query().with({
            "action": ["list"],
            "host_metadata": ["all"],
            "show_qds": ["1"],
            "show_qds_factors": ["1"],
            "truncation_limit": [string(state.batch_size)],
          }).format_query()
      ).with({
          "Header":{
              "X-Requested-With": ["curl"],
              "Authorization": ["Basic "+(state.user+":"+state.password).base64()],
          }
      }).do_request().as(resp,
        resp.StatusCode == 200 
        ?
          (
            has(resp.Body) && size(resp.Body) != 0
            ?
              (resp.Body.as(xml, try(xml.decode_xml('qualys_api_2_0_ahd'), "decode_xml_error_ahd").as(ahd_body, 
                !has(ahd_body.decode_xml_error_ahd) 
                ?
                  (ahd_body.?doc.HOST_LIST_VM_DETECTION_OUTPUT.RESPONSE.hasValue() 
                  ?
                    ({
                      "worklist": (
                        ahd_body.?doc.HOST_LIST_VM_DETECTION_OUTPUT.RESPONSE.HOST_LIST.HOST.orValue([]).map(h,
                          h.DETECTION_LIST.DETECTION.map(v,
                            h.with({
                              "DETECTION_LIST": v,
                            })
                          )).flatten()
                      ),
                      "next_page": { 
                        ?"link": ahd_body.?doc.HOST_LIST_VM_DETECTION_OUTPUT.RESPONSE.WARNING.URL
                      },
                      "next": 0,
                    })
                  :
                    {
                      "events": {
                        "error": {
                          "message": "xsd and response mismatch in ahd:" + (
                            string(resp.Body)
                          ),
                        },
                      },
                      "want_more": false,
                    }
                  )
                :
                  {
                    "events": {
                      "error": {
                        "message": "decode_xml error in ahd:" + string(ahd_body.decode_xml_error) + ":" + (
                          string(resp.Body)
                        ),
                      },
                    },
                    "want_more": false,
                  }
              )))
            :
              // Qualys sends 200 success but with no response body. 
              // Handling this case as okay.
              {
                "events": [],
                "want_more": false,
              }
          )
        :
          ({
            "events": {
              "error": {
                "code": string(resp.StatusCode),
                "id": string(resp.Status),
                "message": "GET:"+(
                  size(resp.Body) != 0 ?
                    string(resp.Body)
                  :
                    string(resp.Status) + ' (' + string(resp.StatusCode) + ')'
                ),
              },
            },
            "want_more": false,
          })
      )
    )
  ).as(state, state.with(
    !has(state.worklist) ? state : 
      size(state.worklist) > 0 ?
        request("GET", state.url.trim_right("/") + "/api/2.0/fo/knowledge_base/vuln/?" +
          {
            "ids": [string(state.worklist[0].DETECTION_LIST.QID)],
            "action": ["list"],
          }.format_query()
        ).with({
          "Header":{
            "X-Requested-With": ["curl"],
            "Authorization": ["Basic "+(state.user+":"+state.password).base64()],
          }
        }).do_request().as(resp, resp.StatusCode == 200 ?
          resp.Body.as(xml, try(xml.decode_xml('qualys_api_2_0_kb'), "decode_xml_error_kb").as(kb_body, 
            !has(kb_body.decode_xml_error_kb) 
            ?
              ({
                "events": (
                  has(kb_body.doc.KNOWLEDGE_BASE_VULN_LIST_OUTPUT.RESPONSE.VULN_LIST)
                  ?
                    kb_body.doc.KNOWLEDGE_BASE_VULN_LIST_OUTPUT.RESPONSE.VULN_LIST.VULN.map(e, e.with({
                        "CVE_LIST": e.?CVE_LIST.CVE.orValue([]).map(c, c.ID),
                    })).as(kb_list, kb_list[0]).as(kb, [{
                      "message": state.worklist[0].with({
                        "KNOWLEDGE_BASE": kb,
                      }).encode_json()
                    }])
                  :
                    [{
                      "message": state.worklist[0].with({
                        "KNOWLEDGE_BASE": {
                          "response_error": "response IDs values are missing: " + string(kb_body.?doc.KNOWLEDGE_BASE_VULN_LIST_OUTPUT.RESPONSE.ID_SET.orValue([])),
                        }
                      }).encode_json()
                    }]
                ),
                "worklist": tail(state.worklist),
                "want_more": has(state.?next_page.link),
              }).as(to_publish, to_publish.with({
                "want_more": to_publish.want_more || size(to_publish.worklist) != 0,
              })).as(state, 
                state.want_more && size(state.events) == 0 ?
                  state.with({"events": [{"message": "want_more"}]})
                :
                  state
              )
            :
              {
                "events": {
                  "error": {
                    "message": "decode_xml error in kb:" + string(kb_body.decode_xml_error) + ":" + (
                      size(resp.Body) != 0 ?
                        string(resp.Body)
                      :
                        string(resp.Status) + ' (' + string(resp.StatusCode) + ')'
                    ),
                  },
                },
                "want_more": false,
              }
          ))
        :
          {
            "events": {
              "error": {
                "code": string(resp.StatusCode),
                "id": string(resp.Status),
                "message": "GET:"+(
                  size(resp.Body) != 0 ?
                    string(resp.Body)
                  :
                    string(resp.Status) + ' (' + string(resp.StatusCode) + ')'
                ),
              },
            },
            "want_more": false,
          }
        )
      :
        {
          "events": [],
          "want_more": false,
          "next_page": {},
        }
    )
  )
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#if preserve_duplicate_custom_fields}}
  - preserve_duplicate_custom_fields
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if cloud_data}}
  - {{cloud_data}}
{{/if}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

paths:
{{#each paths as |path i|}}
 - {{path}}
{{/each}}
condition: {{ condition }}
prospector.scanner.symlinks: {{ symlinks }}
parsers:
- container:
    stream: stdout
    format: cri
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
prospector.scanner.exclude_files:: [".gz$"]
{{#if processors}}
processors:
{{processors}}
{{/if}}
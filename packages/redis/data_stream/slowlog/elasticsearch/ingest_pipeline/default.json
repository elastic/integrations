{
  "description": "Pipeline for parsing redis slowlog logs",
  "processors": [],
  "on_failure" : [
    {
      "set" : {
        "field" : "event.kind",
        "value" : "pipeline_error"
      }
    },
    {
      "append" : {
        "field" : "error.message",
        "value" : "{{{ _ingest.on_failure_message }}}"
      }
    }
  ]
}

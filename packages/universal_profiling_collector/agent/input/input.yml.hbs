pf-elastic-collector:
    host: {{host}}
    verbose: {{verbose}}
    telemetry: {{telemetry}}
    {{#if memory_limit}}
    memory_limit: {{memory_limit}}
    {{/if}}
    auth:
        secret_token: {{secret_token}}
    ssl:
        enabled: {{tls_enabled}}
        certificate: {{tls_certificate_path}}
        key: {{tls_key_path}}
        key_passphrase: {{tls_key_passphrase}}
        supported_protocols:
        {{#each tls_supported_protocols}}
          - {{this}}
        {{/each}}
        cipher_suites:
        {{#each tls_cipher_suites}}
          - {{this}}
        {{/each}}
        curve_types:
        {{#each tls_curve_types}}
          - {{this}}
        {{/each}}

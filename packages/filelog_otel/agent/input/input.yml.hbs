receivers:
  filelog:
    include:
{{#each include}}
      - {{this}}
{{/each}}
{{#if exclude}}
    exclude:
{{#each exclude}}
      - {{this}}
{{/each}}
{{/if}}
    start_at: {{start_at}}
    include_file_name: {{include_file_name}}
    include_file_path: {{include_file_path}}
    poll_interval: {{poll_interval}}

service:
  pipelines:
    logs:
      receivers: [filelog]

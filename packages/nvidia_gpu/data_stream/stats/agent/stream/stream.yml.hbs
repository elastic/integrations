hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
period: {{period}}
use_types: true
rate_counters: false
username: {{username}}
password: {{password}}
metrics_filters.exclude:
{{#each metrics_filters.exclude}}
  - {{this}}
{{/each}}
metrics_filters.include:
{{#each metrics_filters.include}}
  - {{this}}
{{/each}}
{{#if ssl.certificate_authorities}}
ssl:
  {{ssl}}
{{/if}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

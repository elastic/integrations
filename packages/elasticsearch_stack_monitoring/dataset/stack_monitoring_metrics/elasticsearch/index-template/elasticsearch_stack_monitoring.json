{
  "index_patterns": [
    "metrics-elasticsearch_stack_monitoring-*"
  ],
  "priority": 10,
  "data_stream": {
    "timestamp_field": "@timestamp"
  },
  "template": {
    "settings": {
      "index": {
        "lifecycle": {
          "name": "elasticsearch_stack_monitoring"
        }
      }
    },
    "mappings": {
      "properties": {
        "@timestamp": {
          "type": "date"
        }
      }
    }
  }
}
paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}
exclude_files: [".gz$"]
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#if preserve_cef}}
  - preserve_cef
{{/if}}
{{#if preserve_log}}
  - preserve_log
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
processors:
{{#if preserve_original_event}}
- dissect:
    tokenizer: "%{pre_cef} CEF:0"
    field: message
    target_prefix: _tmp
    ignore_failure: true
- copy_fields:
    ignore_missing: true
    fields:
      - from: message
        to: event.original
{{/if}}
- script:
    lang: javascript
    source: function process(event) { event.Put("message", event.Get("message").replace(/\b\\\b/g,"\\\\")); }
- decode_cef:
    field: message
    target: cef
    ecs: false
{{#if tz_offset}}
    timezone: {{tz_offset}}
{{/if}}
{{#if processors}}
{{processors}}
{{/if}}

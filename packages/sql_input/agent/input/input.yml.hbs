metricsets: ["query"]
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
{{#if condition}}
condition: {{ condition }}
{{/if}}
driver: {{driver}}
{{#if sql_query}}
sql_query: "{{sql_query}}"
{{else}}
sql_queries: {{sql_queries}}
{{/if}}
raw_data.enabled: true
period: {{period}}
merge_results: {{merge_results}}
{{#if sql_response_format}}
sql_response_format: {{sql_response_format}}
{{/if}}
{{#if cursor}}
cursor:
    {{cursor}}
{{/if}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
data_stream:
  dataset: {{data_stream.dataset}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

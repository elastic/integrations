metricsets: ["collector"]
hosts:
  - {{host}}
period: {{period}}
metrics_path: /metrics
metrics_filters:
  include: ["was_servlet*"]
{{#if username}}
username: "{{username}}"
{{/if}}
{{#if password}}
password: "{{password}}"
{{/if}}
{{#if ssl}}
{{ssl}}
{{/if}}
tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

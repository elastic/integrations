metricsets: ["json"]
namespace: "bucket"
hosts:
{{#each host_couchbase}}
  - "{{this}}/pools/default/buckets"
{{/each}}
{{#if ssl_couchbase}}
ssl:
  {{ssl_couchbase}}
{{/if}}
{{#if username}}
username: {{username}}
{{/if}}
{{#if password}}
password: {{password}}
{{/if}}
period: {{period}}
json.is_array: true
tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

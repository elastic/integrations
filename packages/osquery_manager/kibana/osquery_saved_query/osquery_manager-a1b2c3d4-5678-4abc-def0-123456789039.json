        "id": "windows_privilege_use_events_elastic",
        "interval": "60",
        "platform": "windows",
        "query": "SELECT time, datetime, source, provider_name, eventid, data FROM windows_events WHERE channel = 'Security' AND eventid IN (4672, 4673, 4674) AND (data LIKE '%SeDebugPrivilege%' OR data LIKE '%SeLoadDriverPrivilege%' OR data LIKE '%SeTakeOwnershipPrivilege%' OR data LIKE '%SeRestorePrivilege%' OR data LIKE '%SeBackupPrivilege%' OR data LIKE '%SeImpersonatePrivilege%' OR data LIKE '%SeAssignPrimaryTokenPrivilege%') ORDER BY time DESC;",

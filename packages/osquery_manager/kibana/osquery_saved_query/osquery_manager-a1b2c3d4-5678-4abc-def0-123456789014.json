        "id": "userassist_execution_elastic",
        "interval": "3600",
        "platform": "windows",
        "query": "SELECT path, count, last_execution_time, sid FROM userassist WHERE (path LIKE '%.exe' OR path LIKE '%.scr' OR path LIKE '%.com' OR path LIKE '%.bat' OR path LIKE '%.ps1' OR path LIKE '%.vbs') AND (path NOT LIKE 'C:\\Windows\\System32\\%' OR path LIKE '%\\Users\\%' OR path LIKE '%\\Temp\\%' OR path LIKE '%\\AppData\\%') ORDER BY last_execution_time DESC LIMIT 100;",

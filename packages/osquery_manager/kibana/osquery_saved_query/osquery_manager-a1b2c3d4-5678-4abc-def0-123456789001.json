        "id": "shimcache_execution_elastic",
        "interval": "3600",
        "platform": "windows",
        "query": "SELECT modified_time, path, executable, execution_flag FROM shimcache WHERE (path LIKE '%\\Users\\%\\AppData\\%' OR path LIKE '%\\Temp\\%' OR path LIKE '%\\Users\\Public\\%' OR path LIKE '%\\ProgramData\\%') AND path NOT LIKE '%\\Windows\\System32\\%' AND path NOT LIKE '%\\Windows\\SysWOW64\\%' ORDER BY modified_time DESC LIMIT 500;",

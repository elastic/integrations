    "attributes": {
        "created_at": "2025-11-07T00:00:00.000Z",
        "created_by": "elastic",
        "description": "Collects browser history from Google Chrome on Windows. Requires ATC (Auto Table Construction) configuration in osquery.conf to create the chrome_history_windows table. IMPORTANT: Chrome must be closed or use VSS/file copy for locked databases. Returns URL history with visit timestamps, page titles, and visit frequency. For other browsers (Edge, Brave, Opera), configure additional ATC tables and modify this query.",
        "ecs_mapping": [
            {
                "key": "url.full",
                "value": {
                    "field": "url"
                }
            },
            {
                "key": "@timestamp",
                "value": {
                    "field": "last_visited"
                }
            },
            {
                "key": "event.action",
                "value": {
                    "field": "title"
                }
            },
            {
                "key": "event.count",
                "value": {
                    "field": "visit_count"
                }
            },
            {
                "key": "user_agent.name",
                "value": {
                    "value": "chrome"
                }
            },
            {
                "key": "event.category",
                "value": {
                    "value": "web"
                }
            },
            {
                "key": "event.type",
                "value": {
                    "value": "info"
                }
            }
        ],
        "id": "browser_history_windows_elastic",
        "interval": "3600",
        "platform": "windows",
        "query": "SELECT\n  last_visited,\n  url,\n  title,\n  visit_count,\n  'chrome' AS browser\nFROM\n  chrome_history_windows\nORDER BY\n  last_visited DESC;",

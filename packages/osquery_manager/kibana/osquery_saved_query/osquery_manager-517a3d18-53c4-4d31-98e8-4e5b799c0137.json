    "id": "lnk_files_recent_activity_elastic",
    "interval": "3600",
    "platform": "windows",
    "query": "SELECT \n  path, \n  filename, \n  size, \n  mtime, \n  atime, \n  ctime \nFROM file \nWHERE (\n    path LIKE 'C:\\\\Users\\\\%\\\\Recent\\\\%.lnk' \n    OR path LIKE 'C:\\\\Users\\\\%\\\\Desktop\\\\%.lnk' \n    OR path LIKE 'C:\\\\Users\\\\%\\\\AppData\\\\Roaming\\\\Microsoft\\\\Windows\\\\Start Menu\\\\Programs\\\\Startup\\\\%.lnk' \n    OR path LIKE 'C:\\\\ProgramData\\\\Microsoft\\\\Windows\\\\Start Menu\\\\Programs\\\\StartUp\\\\%.lnk'\n  ) \n  AND filename LIKE '%.lnk' \nORDER BY mtime DESC \nLIMIT 200;",

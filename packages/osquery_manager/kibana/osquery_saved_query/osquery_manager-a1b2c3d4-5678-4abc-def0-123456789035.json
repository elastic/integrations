        "id": "macos_launchagents_persistence_elastic",
        "interval": "3600",
        "platform": "darwin",
        "query": "SELECT name, label, path, program, program_arguments, run_at_load, keep_alive, inetd_compatibility, start_on_mount, disabled FROM launchd WHERE path LIKE '%/LaunchAgents/%' AND (run_at_load = 1 OR keep_alive = 1 OR start_on_mount = 1 OR program LIKE '/tmp/%' OR program LIKE '%/Library/Application Support/%' OR program LIKE '%curl%' OR program LIKE '%bash -c%' OR program LIKE '%python%' OR program_arguments LIKE '%curl%' OR program_arguments LIKE '%wget%') ORDER BY disabled ASC, run_at_load DESC;",

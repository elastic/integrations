        "id": "registry_persistence_elastic",
        "interval": "3600",
        "platform": "windows",
        "query": "SELECT \n  path, \n  name, \n  data, \n  type \nFROM registry \nWHERE (\n    path LIKE 'HKEY_LOCAL_MACHINE\\\\Software\\\\Microsoft\\\\Windows\\\\CurrentVersion\\\\Run%' \n    OR path LIKE 'HKEY_CURRENT_USER\\\\Software\\\\Microsoft\\\\Windows\\\\CurrentVersion\\\\Run%' \n    OR path LIKE 'HKEY_LOCAL_MACHINE\\\\Software\\\\WOW6432Node\\\\Microsoft\\\\Windows\\\\CurrentVersion\\\\Run%'\n  ) \n  AND name != '' \n  AND data NOT LIKE '%\\\\Windows\\\\System32\\\\%' \n  AND data NOT LIKE '%\\\\Program Files\\\\Windows%' \nORDER BY path \nLIMIT 100;",
        "updated_at": "2025-01-29T12:00:00.000Z",
        "updated_by": "elastic"
    },

        "id": "shellbags_folder_access_elastic",
        "interval": "3600",
        "platform": "windows",
        "query": "SELECT path, accessed_time, modified_time, created_time, sid, source FROM shellbags WHERE (path LIKE '%\\Users\\%\\Desktop%' OR path LIKE '%\\Users\\%\\Downloads%' OR path LIKE '\\\\%' OR path LIKE '%Removable Disk%') AND accessed_time > datetime('now', '-30 days') ORDER BY accessed_time DESC LIMIT 500;",

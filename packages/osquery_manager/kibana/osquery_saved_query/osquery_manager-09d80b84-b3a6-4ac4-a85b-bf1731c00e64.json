    "id": "disk_drives_removable_windows_elastic",
    "interval": "3600",
    "platform": "windows",
    "query": "SELECT \n  device_id, \n  description, \n  file_system, \n  size, \n  free_space, \n  boot_partition \nFROM logical_drives \nWHERE size > 0 \n  AND (\n    file_system IN ('FAT', 'FAT32', 'exFAT', 'CDFS', 'UDF') \n    OR boot_partition = 0\n  ) \nORDER BY device_id;",

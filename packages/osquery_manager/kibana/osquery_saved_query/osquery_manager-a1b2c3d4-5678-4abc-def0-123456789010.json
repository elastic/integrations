        "id": "network_connections_c2_elastic",
        "interval": "3600",
        "query": "SELECT pid, protocol, local_address, local_port, remote_address, remote_port, state FROM process_open_sockets WHERE state = 'ESTABLISHED' AND remote_address NOT LIKE '10.%' AND remote_address NOT LIKE '192.168.%' AND remote_address NOT LIKE '172.16.%' AND remote_address NOT LIKE '172.17.%' AND remote_address NOT LIKE '172.18.%' AND remote_address NOT LIKE '172.19.%' AND remote_address NOT LIKE '172.2_.%' AND remote_address NOT LIKE '172.30.%' AND remote_address NOT LIKE '172.31.%' AND remote_address != '127.0.0.1' AND remote_address != '::1' AND remote_address != '' ORDER BY remote_port LIMIT 100;",
        "updated_at": "2025-01-29T12:00:00.000Z",

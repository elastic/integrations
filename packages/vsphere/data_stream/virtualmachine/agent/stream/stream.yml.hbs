metricsets: ["virtualmachine"]
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
period: {{period}}
username: {{username}}
password: {{password}}
# If insecure is true, don't verify the server's certificate chain
insecure: {{insecure}}
# Get custom fields when using virtualmachine metric set. Default false.
get_custom_fields: {{get_custom_fields}}
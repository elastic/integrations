metricsets: ["network"]
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
period: {{period}}
username: {{username}}
password: {{password}}
# If insecure is true, don't verify the server's certificate chain
insecure: {{insecure}}

tags:
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}

{{#if processors}}
processors:
{{processors}}
{{/if}}
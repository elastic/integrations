metricsets: ["json"]
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
{{#if basepath}}
basepath: {{basepath}}
{{/if}}
{{#if username}}
username: {{username}}
{{/if}}
{{#if password}}
password: {{password}}
{{/if}}
period: {{period}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
namespace: "background_task_utilization"
path: "/api/task_manager/_background_task_utilization"
method: "GET"
processors:
  - rename:
      fields:
        - from: http.background_task_utilization
          to: kibana.background_task_utilization
      ignore_missing: true
      fail_on_error: false


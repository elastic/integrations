host: "{{tcp_host}}:{{tcp_port}}"
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
{{#if ssl}}
ssl: {{ssl}}
{{/if}}
processors:
- add_locale: ~
{{#if processors}}
{{processors}}
{{/if}}
{{#if tcp_options}}
{{tcp_options}}
{{/if}}
fields_under_root: true
fields:
  _conf:
    tz_offset: '{{tz_offset}}'

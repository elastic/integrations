paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}
exclude_files: [".gz$"]

tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}

{{#if redact_passwords}}
  - redact_passwords
{{/if}}

{{#each tags as |tag|}}
  - {{tag}}
{{/each}}

processors:
  - add_locale: ~
  - add_host_metadata: ~
{{#if processors}}
{{processors}}
{{/if}}

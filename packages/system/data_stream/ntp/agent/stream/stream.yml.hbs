metricsets: ["ntp"]
period: {{period}}
ntp.servers:
{{#each ntp.servers}}
  - {{this}}
{{/each}}
{{#if ntp.timeout}}
ntp.timeout: {{ntp.timeout}}
{{/if}}
{{#if ntp.version}}
ntp.version: {{ntp.version}}
{{/if}}
{{#if processors.length}}
processors:
{{processors}}
{{/if}}
{{#if tags.length}}
tags:
{{#each tags as |tag i|}}
- {{tag}}
{{/each}}
{{/if}}

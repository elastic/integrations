paths:
{{#each paths as |path i|}}
 - {{path}}
{{/each}}
exclude_files: [".gz$"]
multiline:
  pattern: "^\\s"
  match: after
processors:
- add_locale: ~
{{#if processors.length}}
{{processors}}
{{/if}}
{{#if tags.length}}
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{/if}}
{{#if ignore_older}}
ignore_older: {{ignore_older}}
{{/if}}
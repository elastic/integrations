{{#if condition }}
condition: {{condition}}
{{/if}}

paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}

exclude_files: 
{{#each exclude_files as |exclude_files|}}
  - {{exclude_files}}
{{/each}}

multiline:
  pattern: "^\\s"
  match: after

processors:
- add_locale: ~
{{#if processors.length}}
{{processors}}
{{/if}}
allow_deprecated_use: true
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}

{{#if ignore_older}}
ignore_older: {{ignore_older}}
{{/if}}

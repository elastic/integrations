metricsets: ["core"]
core.metrics:
{{#each core.metrics}}
- {{this}}
{{/each}}
{{#if system.hostfs}}
system.hostfs: {{system.hostfs}}
{{/if}}
{{#if use_performance_counters}}
use_performance_counters: {{use_performance_counters}}
{{/if}}
{{#if processors.length}}
processors:
{{processors}}
{{/if}}
{{#if tags.length}}
tags:
{{#each tags as |tag i|}}
- {{tag}}
{{/each}}
{{/if}}
period: {{period}}
name: Security
condition: ${host.platform} == 'windows'
{{#if event_id}}
event_id: {{event_id}}
{{/if}}
{{#if ignore_older}}
ignore_older: {{ignore_older}}
{{/if}}
{{#if language}}
language: {{language}}
{{/if}}
{{#if tags.length}}
tags:
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{/if}}
{{#if preserve_original_event}}
include_xml: true
{{/if}}
{{#if processors.length}}
processors:
{{processors}}
{{/if}}

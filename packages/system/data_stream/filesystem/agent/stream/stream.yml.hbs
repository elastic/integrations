metricsets: ["filesystem"]
period: {{period}}
processors: {{processors}}
{{#if filesystem.ignore_types}}
filesystem.ignore_types: 
{{#each filesystem.ignore_types as |type i|}}
- {{type}}
{{/each}}
{{/if}}
{{#if system.hostfs}}
system.hostfs: {{system.hostfs}}
{{/if}}
{{#if tags.length}}
tags:
{{#each tags as |tag i|}}
- {{tag}}
{{/each}}
{{/if}}
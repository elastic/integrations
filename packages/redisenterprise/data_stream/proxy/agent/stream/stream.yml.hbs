metricsets: ["collector"]
period: {{period}}

# disable
use_types: false
rate_counters: false

hosts:
{{#each hosts}}
  - {{this}}
{{/each}}

# Filters
metrics_filters.include : ["listener_*"]
metrics_filters.exclude: ["^up$"]

# Empty path, since prometheus is available on seprate port.
metrics_path:

ssl.verification_mode: "none"

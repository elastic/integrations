metricsets: ["collector"]
period: {{period}}

hosts:
{{#each hosts}}
  - {{this}}
{{/each}}

# Filters
metrics_filters.include : ["node_*"]
metrics_filters.exclude: ["^up$"]

# Empty path, since prometheus is available on seprate port.
metrics_path:

ssl.verification_mode: "none"
use_types: true
{{#if processors}}
processors:
{{processors}}
{{/if}}

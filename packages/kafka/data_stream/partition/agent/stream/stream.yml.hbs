metricsets: ["partition"]
period: {{period}}
hosts:
{{#each hosts}}
  - {{this}}
{{/each}}
{{#if username}}
username: {{username}}
{{/if}}
{{#if password}}
password: {{password}}
{{/if}}
{{#if ssl.certificate}}
ssl.certificate: {{ssl.certificate}}
{{/if}}
{{#if ssl.certificate_authorities}}
ssl.certificate_authorities: {{ssl.certificate_authorities}}
{{/if}}
{{#if ssl.key}}
ssl.key: {{ssl.key}}
{{/if}}

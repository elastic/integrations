{
    "attributes": {
        "author": [
            "Elastic"
        ],
        "building_block_type": "default",
        "description": "Adversaries may use built-in applications to get a listing of local system or domain accounts and groups.",
        "from": "now-119m",
        "history_window_start": "now-5d",
        "index": [
            "logs-endpoint.events.*"
        ],
        "interval": "60m",
        "language": "kuery",
        "license": "Elastic License v2",
        "name": "Account or Group Discovery via Built-In Tools",
        "new_terms_fields": [
            "process.parent.executable",
            "process.command_line",
            "agent.id"
        ],
        "query": "event.category:process and host.os.type:(linux or macos) and event.action:exec and (\n  (process.name:\"dscl\" and process.args:(/Active*Directory/* or /Users* or /Groups*)) or\n  (process.name:\"dscacheutil\" and process.args:(\"user\" or \"group\")) or\n  (process.name:\"getent\" and process.args:(\"passwd\" or \"group\") and process.args_count >= 3)\n) and not (\n  process.parent.executable:(\n    \"/usr/lib/ocf/resource.d/cyanconnode/mysql\" or /snap/* or \"/usr/bin/check_mk_agent\" or \"/usr/bin/cloudflared\" or\n    /opt/puppetlabs/* or /var/lib/snapd/* or \"/usr/lib/google/guest_agent/core_plugin\" or \"/opt/rudder/bin/cf-agent\" or\n    \"/opt/NinjaRMMAgent/programfiles/ninjarmm-linagent\"\n  ) or\n  process.working_directory:(\n    /var/snap/* or  \"/Applications/NinjaRMMAgent/programfiles/ninjarmm-macagent\" or \"/var/lib/nanitor/agent\" or\n    \"/var/ossec\" or /opt/rapid7/* or \"/opt/Tanium/TaniumClient\"\n  )\n)\n",
        "related_integrations": [
            {
                "package": "endpoint",
                "version": "^8.2.0"
            }
        ],
        "required_fields": [
            {
                "ecs": true,
                "name": "event.action",
                "type": "keyword"
            },
            {
                "ecs": true,
                "name": "event.category",
                "type": "keyword"
            },
            {
                "ecs": true,
                "name": "host.os.type",
                "type": "keyword"
            },
            {
                "ecs": true,
                "name": "process.args",
                "type": "keyword"
            },
            {
                "ecs": true,
                "name": "process.args_count",
                "type": "long"
            },
            {
                "ecs": true,
                "name": "process.name",
                "type": "keyword"
            },
            {
                "ecs": true,
                "name": "process.parent.executable",
                "type": "keyword"
            },
            {
                "ecs": true,
                "name": "process.working_directory",
                "type": "keyword"
            }
        ],
        "risk_score": 21,
        "rule_id": "f638a66d-3bbf-46b1-a52c-ef6f39fb6caf",
        "severity": "low",
        "tags": [
            "Domain: Endpoint",
            "OS: Linux",
            "OS: macOS",
            "Use Case: Threat Detection",
            "Tactic: Discovery",
            "Rule Type: BBR",
            "Data Source: Elastic Defend"
        ],
        "threat": [
            {
                "framework": "MITRE ATT&CK",
                "tactic": {
                    "id": "TA0007",
                    "name": "Discovery",
                    "reference": "https://attack.mitre.org/tactics/TA0007/"
                },
                "technique": [
                    {
                        "id": "T1069",
                        "name": "Permission Groups Discovery",
                        "reference": "https://attack.mitre.org/techniques/T1069/",
                        "subtechnique": [
                            {
                                "id": "T1069.001",
                                "name": "Local Groups",
                                "reference": "https://attack.mitre.org/techniques/T1069/001/"
                            },
                            {
                                "id": "T1069.002",
                                "name": "Domain Groups",
                                "reference": "https://attack.mitre.org/techniques/T1069/002/"
                            }
                        ]
                    },
                    {
                        "id": "T1087",
                        "name": "Account Discovery",
                        "reference": "https://attack.mitre.org/techniques/T1087/",
                        "subtechnique": [
                            {
                                "id": "T1087.001",
                                "name": "Local Account",
                                "reference": "https://attack.mitre.org/techniques/T1087/001/"
                            },
                            {
                                "id": "T1087.002",
                                "name": "Domain Account",
                                "reference": "https://attack.mitre.org/techniques/T1087/002/"
                            }
                        ]
                    }
                ]
            }
        ],
        "timestamp_override": "event.ingested",
        "type": "new_terms",
        "version": 6
    },
    "id": "f638a66d-3bbf-46b1-a52c-ef6f39fb6caf_6",
    "type": "security-rule"
}
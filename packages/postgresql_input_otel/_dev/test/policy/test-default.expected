connectors:
    forward: {}
exporters:
    elasticsearch/componentid-0: {}
inputs: []
output_permissions:
    default:
        _elastic_agent_checks:
            cluster:
                - monitor
        _elastic_agent_monitoring:
            indices: []
        uuid-for-permissions-on-related-indices:
            indices:
                - names:
                    - metrics-*-*
                  privileges:
                    - auto_configure
                    - create_doc
processors:
    transform/componentid-0:
        metric_statements:
            - context: datapoint
              statements:
                - set(attributes["data_stream.type"], "metrics")
                - set(attributes["data_stream.dataset"], "postgresql_input_otel.postgresqlreceiver")
                - set(attributes["data_stream.namespace"], "ep")
receivers:
    postgresql/componentid-0:
        collection_interval: 10s
        connection_pool:
            max_idle: 2
            max_idle_time: 10m
            max_lifetime: 0
            max_open: 5
        endpoint: localhost:5432
        events:
            db.server.query_sample:
                enabled: true
            db.server.top_query:
                enabled: true
        initial_delay: 1s
        metrics:
            postgresql.blks_hit:
                enabled: true
            postgresql.blks_read:
                enabled: true
            postgresql.database.locks:
                enabled: true
            postgresql.tup_deleted:
                enabled: true
            postgresql.tup_fetched:
                enabled: true
            postgresql.tup_inserted:
                enabled: true
            postgresql.tup_returned:
                enabled: true
            postgresql.tup_updated:
                enabled: true
        password: postgresql
        query_sample_collection:
            max_rows_per_query: 100
        tls:
            insecure: true
        top_query_collection:
            max_rows_per_query: 100
            top_n_query: 100
        transport: tcp
        username: postgresql
secret_references: []
service:
    pipelines:
        logs:
            exporters:
                - elasticsearch/componentid-0
            receivers:
                - forward
        logs/componentid-0:
            exporters:
                - forward
            processors:
                - transform/componentid-0
            receivers:
              - postgresql/componentid-0
        metrics:
            exporters:
                - elasticsearch/componentid-0
            receivers:
                - forward
        metrics/componentid-1:
            exporters:
                - forward
            processors:
                - transform/componentid-0
            receivers:
                - postgresql/componentid-0
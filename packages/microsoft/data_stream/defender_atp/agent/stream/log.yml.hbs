paths:
{{#each paths as |path i|}}
  - {{path}}
{{/each}}
exclude_files: [".gz$"]
tags:
{{#each tags as |tag i|}}
 - {{tag}}
{{/each}}
fields_under_root: true
fields:
    observer:
        vendor: "Microsoft"
        product: "DHCP"
        type: "Application"
{{#contains tags "forwarded"}}
publisher_pipeline.disable_host: true
{{/contains}}

paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}
prospector.scanner.exclude_files: ['\.gz$']
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}
processors:
- add_locale: ~
{{#if reroute_config}}
{{reroute_config}}
{{/if}}
{{#if processors}}
{{processors}}
{{/if}}
{{#if filestream_options}}
{{filestream_options}}
{{/if}}
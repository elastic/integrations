udp:
host: "{{udp_host}}:{{udp_port}}"
{{#if udp_options}}
{{udp_options}}
{{/if}}
tags:
{{#if preserve_original_event}}
- preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
- {{tag}}
{{/each}}
fields_under_root: true
{{#contains "forwarded" tags}}
publisher_pipeline.disable_host: true
{{/contains}}

processors:
- add_locale: ~
{{#if processors}}
{{processors}}
{{/if}}